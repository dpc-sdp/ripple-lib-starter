{
  "tasks": {
    "build": {
      "name": "build",
      "steps": [
        {
          "exec": "nuxi build .playground"
        }
      ]
    },
    "cy:open": {
      "name": "cy:open",
      "steps": [
        {
          "exec": "cypress open"
        }
      ]
    },
    "cy:run": {
      "name": "cy:run",
      "steps": [
        {
          "exec": "cypress run"
        }
      ]
    },
    "default": {
      "name": "default",
      "description": "Synthesize project files",
      "steps": [
        {
          "exec": "ts-node --project tsconfig.dev.json .projenrc.ts"
        }
      ]
    },
    "dev": {
      "name": "dev",
      "steps": [
        {
          "exec": "nuxi prepare & nuxi dev .playground"
        }
      ]
    },
    "dev:mock": {
      "name": "dev:mock",
      "env": {
        "NUXT_PUBLIC_API_URL": "http://localhost:3001",
        "API_PORT": "3001"
      },
      "steps": [
        {
          "exec": "npm run dev"
        }
      ]
    },
    "eject": {
      "name": "eject",
      "description": "Remove projen from the project",
      "env": {
        "PROJEN_EJECTING": "true"
      },
      "steps": [
        {
          "spawn": "default"
        }
      ]
    },
    "install": {
      "name": "install",
      "description": "Install project dependencies and update lockfile (non-frozen)",
      "steps": [
        {
          "exec": "npm install"
        }
      ],
      "condition": "[ -z \"$PROJEN_SKIP_INSTALL\" ]"
    },
    "install:ci": {
      "name": "install:ci",
      "description": "Install project dependencies using frozen lockfile",
      "steps": [
        {
          "exec": "npm ci"
        }
      ]
    },
    "lagoon:env": {
      "name": "lagoon:env",
      "description": "Copy environment variables from Lagoon into .playground/.env. Requires lagoon CLI installed.",
      "steps": [
        {
          "exec": "./scripts/lagoon-env.sh --output ./.playground/.env reference-sdp-vic-gov-au"
        }
      ]
    },
    "lint": {
      "name": "lint",
      "steps": [
        {
          "exec": "eslint --config ./.playground/eslint.config.mjs"
        }
      ]
    },
    "preview": {
      "name": "preview",
      "steps": [
        {
          "exec": "nuxi preview .playground"
        }
      ]
    },
    "start": {
      "name": "start",
      "steps": [
        {
          "exec": "nuxi start .playground"
        }
      ]
    },
    "start:mock": {
      "name": "start:mock",
      "env": {
        "NUXT_PUBLIC_API_URL": "http://localhost:3001",
        "API_PORT": "3001"
      },
      "steps": [
        {
          "exec": "npm run start"
        }
      ]
    },
    "test": {
      "name": "test",
      "description": "Run tests"
    },
    "test:integration": {
      "name": "test:integration",
      "steps": [
        {
          "exec": "NUXT_PUBLIC_TIDE_SITE=TEST_SITE NUXT_PUBLIC_TIDE_BASE_URL=https://test.base.url/ start-test dev:mock tcp:3000 'cy:open'"
        }
      ]
    },
    "test:integration-ci": {
      "name": "test:integration-ci",
      "steps": [
        {
          "exec": "NUXT_PUBLIC_TIDE_SITE=TEST_SITE NUXT_PUBLIC_TIDE_BASE_URL=https://test.base.url/ start-test start:mock tcp:3000 'cy:run'"
        }
      ]
    },
    "test:unit": {
      "name": "test:unit",
      "steps": [
        {
          "exec": "vitest run"
        }
      ]
    },
    "upgrade": {
      "name": "upgrade",
      "description": "upgrade dependencies",
      "env": {
        "CI": "0"
      },
      "steps": [
        {
          "exec": "npx npm-check-updates@16 --upgrade --target=minor --peer --no-deprecated --dep=dev,peer,prod,optional --filter=@dpc-sdp/ripple-sdp-projen-config,@types/node,constructs,jscodeshift,prettier,projen,ts-node,typescript"
        },
        {
          "exec": "npm install"
        },
        {
          "exec": "npm update @dpc-sdp/nuxt-ripple-analytics @dpc-sdp/nuxt-ripple-preview @dpc-sdp/nuxt-ripple @dpc-sdp/ripple-sdp-core @dpc-sdp/ripple-sdp-projen-config @dpc-sdp/ripple-test-utils @nuxt/eslint @types/node constructs cypress eslint jscodeshift nuxt prettier projen start-server-and-test ts-node typescript vitest"
        },
        {
          "exec": "npx projen"
        },
        {
          "spawn": "post-upgrade"
        }
      ]
    }
  },
  "env": {
    "PATH": "$(npx -c \"node --print process.env.PATH\")"
  },
  "//": "~~ Generated by projen. To modify, edit .projenrc.ts and run \"npx projen\"."
}
