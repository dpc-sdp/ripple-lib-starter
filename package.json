{
  "name": "nuxt-ripple-starter",
  "version": "0.0.1",
  "main": "./nuxt.config.ts",
  "scripts": {
    "dev": "nuxi prepare & nuxi dev .playground",
    "dev:mock": "NUXT_PUBLIC_API_URL=http://localhost:3001 concurrently 'npm run mockserver' 'npm run dev'",
    "mockserver": "node ./mockserver.mjs",
    "build": "nuxi build .playground",
    "generate": "nuxi generate .playground",
    "preview": "nuxi preview .playground",
    "lint": "eslint .",
    "cy:open": "cypress open",
    "test:cy": "start-test 'NUXT_PUBLIC_API_URL=http://localhost:3001 npm run dev' 'http-get://localhost:3000/api/tide/site?id=8888' 'cy:open'"
  },
  "engines": {
    "node": "^14.16.0 || ^16.11.0"
  },
  "dependencies": {
    "@dpc-sdp/nuxt-ripple": "^2.1.0-alpha.77",
    "@dpc-sdp/ripple-tide-api": "^2.1.0-alpha.76",
    "@dpc-sdp/ripple-tide-event": "^2.1.0-alpha.76",
    "@dpc-sdp/ripple-tide-grant": "^2.1.0-alpha.76",
    "@dpc-sdp/ripple-tide-landing-page": "^2.1.0-alpha.77",
    "@dpc-sdp/ripple-tide-media": "^2.1.0-alpha.77",
    "@dpc-sdp/ripple-tide-news": "^2.1.0-alpha.77",
    "@dpc-sdp/ripple-tide-publication": "^2.1.0-alpha.77",
    "@dpc-sdp/ripple-tide-search": "^2.1.0-alpha.76",
    "@dpc-sdp/ripple-ui-core": "^2.1.0-alpha.76",
    "@dpc-sdp/ripple-ui-forms": "^2.1.0-alpha.76"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "latest",
    "@bahmutov/cypress-esbuild-preprocessor": "latest",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "concurrently": "^7.6.0",
    "cypress": "^12.5.1",
    "eslint": "^8.28.0",
    "mockttp": "^3.6.3",
    "nuxt": "^3.0.0",
    "start-server-and-test": "^1.15.3",
    "typescript": "^4.9.3"
  }
}
